echo
set ds https://master.dl.sourceforge.net/project/ryanboot/gentoo-minimal/
echo -n Gentoo site: ${}
read ds
echo

iseq ${ds} https://master.dl.sourceforge.net/project/ryanboot/gentoo-minimal/ && set link ?viasf=1 ||

