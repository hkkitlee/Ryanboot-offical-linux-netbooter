#################################################
:colour
echo https://ipxe.org/cmd/cpair
echo

set fc 0
echo -n Foreground colour index: ${}
read fc

set bc 7
echo -n Background colour index: ${}
read bc

set pi 7
echo -n Pair index: ${}
read pi

cpair --foreground ${fc} --background ${bc} ${pi}

set leave y
echo -n Press enter to leave or N/n to do again:${}
read leave
echo
iseq ${answer} n && goto colour ||
iseq ${answer} N && goto colour ||
goto start
