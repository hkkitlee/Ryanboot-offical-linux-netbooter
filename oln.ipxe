echo
set mainv 9
echo http://ftp.cvut.cz/slax/Slax-${mainv}.x/
echo -n Main Version : ${}                                                                                   
read mainv                                                                                                       

set subv 11.0
echo -n Sub Version : ${}                                                                                        
